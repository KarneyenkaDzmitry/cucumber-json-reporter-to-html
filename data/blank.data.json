{
    "html": "<!DOCTYPE html><html lang=\"en\">#head#body</html>",
    "head": [
        "<head>",
        "<meta charset=\"UTF-8\">",
        "<meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">",
        "<link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\">",
        "<script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js\"></script>",
        "<script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js\"></script>",
        "<title>#title</title>",
        "<link rel=\"stylesheet\" href=\"#cssPath\" type=\"text/css\" />",
        "</head>"
    ],
    "body": {
        "header": [
            "<body><header>",
            "<nav class=\"navbar navbar-default navbar-static-top\" role=\"navigation\">",
            "<div class=\"container\">",
            "<div class=\"navbar-brand\">#title</div>",
            "<div class=\"navbar-header label-container\"><p>",
            "<span class=\"label label-success\" title=\"Tests\">Passed: #passedAmount</span>",
            "<span class=\"label label-danger\" title=\"Tests\">Failed: #failedAmount</span>",
            "<span class=\"label label-warning\" title=\"Tests\">Skipped: #skippedAmount</span>",
            "</p></div></div></nav></header>"
        ],
        "main": [
            "<main><div class=\"body-results-wrapper\">",
            "<div class=\"container\">#description</div>",
            "<div class=\"content\">",
            "<div class=\"header-container\">",
            "<div class=\"container\">",
            "<ul class=\"nav nav-tabs\">",
            "<li class=\"active\"><a href=\"#all\">All</a></li>",
            "<li><a href=\"#passed\">Passed</a></li>",
            "<li><a href=\"#failed\">Failed</a></li>",
            "</ul><div class=\"tab-content\">",
            "<div id=\"all\" class=\"tab-pane fade in active\">#allFeaturesList</div>",
            "<div id=\"passed\" class=\"tab-pane fade\">#passedFeaturesList</div>",
            " <div id=\"failed\" class=\"tab-pane fade\">#failedFeaturesList</div>",
            "</div></div><script>",
            "$(document).ready(function () {",
            "$(\".nav-tabs a\").click(function () {",
            " $(this).tab('show');",
            "});});</script></div></div></div></main>"
        ],
        "footer": [
            "<footer><div> &nbsp;<br> &nbsp; </div>",
            "<div class=\"navbar-fixed-bottom row-fluid footer-div\">",
            "<div class=\"navbar-inner\">",
            "<div class=\"footer date\">#date</div>",
            "<div class=\"footer-container\">",
            "<a target=\"_blank\" href=\"https://github.com/KarneyenkaDzmitry/cucumber-json-reporter-to-html\">",
            "<div class=\"text-muted footer-link\">",
            "generated by @cucumber-json-reporter-to-html</div></a></div></div></div></footer></body>"
        ],
        "styles": [
            "@media (min-width: 768px) {.pull-right-lg {float: left;}}",
            "@media (min-width: 992px) and (max-width: 1199px) {.pull-right-lg {float: right;}}",
            "@media (min-width: 768px) and (max-width: 991px) {.pull-right-lg {float: right;}}",
            "@media (min-width: 1200px) {.pull-right-lg {float: right;}}",
            ".metadata {overflow: auto;letter-spacing: 0.2px;border-color: white;line-height: 1.6;color: #4d4d4d;",
            "font-family: \"Helvetica Neue\", \"Helvetica\", Helvetica, Arial, sans-serif;font-size: 13px;padding-bottom: 3px;}",
            ".panel-heading {padding: 0;border: 1px solid black;}",
            ".panel-heading a {padding: 10px 15px;display: block;position: relative;text-decoration: none;}",
            ".panel-heading .label-container {position: absolute;top: 8px;right: 8px;}",
            ".panel-group {margin: 0;padding: 0;}",
            ".navbar .label-container {position: absolute;right: 10px;top: 14px;}",
            ".navbar {margin-bottom: 10px;}",
            ".navbar .label {font-size: 20px;}",
            ".navbar .label-skipped {background-color: yellow;}",
            ".navbar .project-name {position: absolute;top: 10px;left: 50%;margin-left: -100px;text-align: center;font-size: 20px;font-weight: bold;}",
            ".screenshot {text-align:right;}",
            ".duration {text-align:right;color: rgb(104,102,102);}",
            ".footer-div {text-align: right;vertical-align: middle;height: 3.5%;width: 100%;}",
            ".footer-container {margin-right: 10px;margin-top: 5px;width: 18%;z-index: 10;position: absolute;right: 0;",
            "bottom: 10px;text-align: center;background-color: transparent;}",
            ".show-modal:hover {background-color: #f0f0f0;}",
            ".footer-link {font-size: 13px;float: right;}",
            ".footer-link:hover {color: darkgray;}",
            ".scenario {padding-left: 5px;padding-right: 5px;border: 1px solid rgb(231,231,231);}",
            ".step{padding-left: 25px;margin-bottom: 0px;}",
            ".step.label-success {background-color: rgb(143,201,143)}",
            ".step.label-danger {background-color: rgb(245,104,104)}",
            ".step.label-warning {background-color:rgb(255,251,39)}",
            "body {background-color: rgb(243,240,233)}",
            ".date {text-align: left;color: rgb(158,150,150);margin-left: 10px;margin-top: -5px}",
            ".row {padding: 2px 2px 0px 0px}"
        ]
    }
}